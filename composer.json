{
    "name": "php/skeleton",
    "description":"Generic PHP.Skeleton package",
    "keywords":[
        "Skeleton",
        "PSR"
    ],
    "homepage":"https://github.com/koriym/PHP.Skeleton",
    "license":"BSD-3-Clause",
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "__Vendor__\\__Package__\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "__Vendor__\\__Package__\\": ["tests/", "tests/Fake"]
        }
    },
    "scripts"   :{
        "post-install-cmd": "__Vendor__\\__Package__\\Installer::postInstall"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
